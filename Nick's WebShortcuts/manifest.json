{
  "manifest_version": 3,
  "name": "Nick's WebShortcuts",
  "version": "1.0",
  "description": "A very simply made chrome extension with shortcuts",
  "action": {
    "default_popup": "Main.html"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "Icons/Icon16.png",
    "48": "Icons/Icon48.png",
    "128": "Icons/Icon128.png"
  }
}
